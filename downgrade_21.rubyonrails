from moment.versioning import api
from config import SQLALCHEMY_DATABASE_URI, SQLALCHEMY_MOMENT_REPO
v = api.db_version(SQLALCHEMY_DATABASE_URI, SQLALCHEMY_MOMENT_REPO)
api.downgrade(SQLALCHEMY_DATABASE_URI, SQLALCHEMY_MOMENT_REPO, v - 1)
print 'Current database version: ' + str(api.db_version(SQLALCHEMY_DATABASE_URI, SQLALCHEMY_MOMENT_REPO))
